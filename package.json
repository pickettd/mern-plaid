{
  "name": "mern-auth",
  "version": "1.0.0",
  "description": "Waiwai Mern Plaid",
  "main": "server.js",
  "scripts": {
    "build": "npm run client-install && npm run build --prefix mlp-client",
    "client-install": "npm install && npm install --prefix mlp-client",
    "start": "node server.js",
    "server": "HOST=0.0.0.0 nodemon --ignore 'mlp-client/' server.js",
    "client": "HOST=0.0.0.0 npm start --prefix mlp-client",
    "dev": "concurrently \"PORT=5000 npm run server\" \"npm run client\""
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "express": "^4.16.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.23.0",
    "mongoose": "^5.3.11",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "plaid": "^2.10.0",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.1"
  },
  "engines": {
    "node": ">=12.0"
  }
}
